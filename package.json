{
  "name": "svelte-repo-card",
  "version": "1.0.0-rc.1",
  "description": "A simple GitHub repo card, based on Tarptaeya/repo-card",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "clean": "rm -rf dist",
    "lint": "prettier -c '**/*'",
    "prepublishOnly": "npm run clean && npm run validate && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "less": "^3.13.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.2",
    "rollup": "^2.38.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.32",
    "svelte-preprocess": "^4.6.6",
    "svelte2dts": "^0.2.12",
    "svelte2tsx": "^0.1.166",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "component",
    "svelte",
    "github",
    "card"
  ],
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thislooksfun/svelte-repo-card.git"
  },
  "author": "thislooksfun <thislooksfun@repbot.org> (https://thislooks.fun)",
  "bugs": {
    "url": "https://github.com/thislooksfun/svelte-repo-card/issues"
  },
  "homepage": "https://github.com/thislooksfun/svelte-repo-card#readme"
}

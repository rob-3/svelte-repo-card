{
  "name": "svelte-repo-card",
  "version": "1.0.0",
  "description": "A simple GitHub repo card, based on Tarptaeya/repo-card",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "test": "jest",
    "clean": "rm -rf dist",
    "lint": "svelte-check && prettier -c '**/*'",
    "prepublishOnly": "pinst --disable && npm run clean && npm run lint && npm run build",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.20",
    "@types/testing-library__jest-dom": "^5.9.5",
    "babel-jest": "^26.6.3",
    "fast-check": "^2.12.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "less": "^3.13.1",
    "nock": "^13.0.7",
    "node-fetch": "^2.6.1",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.3",
    "rollup": "^2.38.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.3.8",
    "svelte": "^3.32.2",
    "svelte-check": "^1.1.33",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.6",
    "svelte2dts": "^0.2.12",
    "svelte2tsx": "^0.1.167",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "component",
    "svelte",
    "github",
    "card"
  ],
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thislooksfun/svelte-repo-card.git"
  },
  "author": "thislooksfun <thislooksfun@repbot.org> (https://thislooks.fun)",
  "bugs": {
    "url": "https://github.com/thislooksfun/svelte-repo-card/issues"
  },
  "homepage": "https://github.com/thislooksfun/svelte-repo-card#readme"
}
